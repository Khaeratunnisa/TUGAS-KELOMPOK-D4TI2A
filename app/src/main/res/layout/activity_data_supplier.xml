<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.hera.tugaskelompok.DataSupplier">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Data Barang | "
        android:id="@+id/button_brg2"
        style="?android:attr/borderlessButtonStyle"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:background="#9ae3f6"
        android:layout_toStartOf="@+id/button_spl2" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Data Supplier | "
        android:id="@+id/button_spl2"
        style="?android:attr/borderlessButtonStyle"
        android:layout_alignTop="@+id/button_brg2"
        android:background="#e9e9e9"
        android:layout_toStartOf="@+id/button_kry2" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Data Karyawan"
        android:id="@+id/button_kry2"
        style="?android:attr/borderlessButtonStyle"
        android:layout_alignTop="@+id/button_spl2"
        android:background="#9ae3f6"
        android:layout_alignEnd="@+id/button_ipspl" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Input Data Supplier"
        android:id="@+id/button_ipspl"
        android:layout_below="@+id/button_kry2"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true" />

    <ListView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/listView3"
        android:layout_below="@+id/button_ipspl"
        android:layout_centerHorizontal="true" />
</RelativeLayout>
